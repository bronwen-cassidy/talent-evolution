<?xml version="1.0" encoding="UTF-8"?>
<xml>
    <questionnaire name="AllQuestions" title="allq">

        <group name="Group Name for Text">
            <question type="Text" name="Text Name" title="Text Title" mandatory="Y"/>
        </group>

        <group name="Group Name for Date">
            <question type="Date" name="Date Name" title="Date Title"/>
        </group>

        <group name="Group Name for Integer">
            <question type="Integer" name="Integer Name" title="Integer Title"/>
        </group>

        <group name="Group Name for Textbox">
            <question type="TextBox" name="Textbox Name" title="Textbox Title"/>
        </group>

        <group name="Group Name for Select">

            <question type="Select" name="Select Name" title="Select Title" mandatory="Y">
                <answer value="Select Value1" text="Select Text1"/>
                <answer value="Select Value2" text="Select Text2"/>
                <answer value="Select Value3" text="Select Text3"/>
                <answer value="Select Value4" text="Select Text4"/>
                <answer value="Select Value5" text="Select Text5"/>
            </question>
        </group>

        <group name="Group Name for Multi Question Test">

            <multi-question name=" ">
                <line-item>Radio 1</line-item>
                <line-item>Radio 2</line-item>

                <question type="Radio" name="Radio Name" title="Radio Title">
                    <answer value="Radio Value 1" text="Radio Text 1"/>
                    <answer value="Radio Value 2" text="Radio Text 2"/>
                    <answer value="Radio Value 3" text="Radio Text 3"/>
                </question>
                <question type="Text" name="Text Name" title="Text Title"/>
            </multi-question>

            <multi-question name="">
                <line-item>Challenge 1</line-item>
                <line-item>Challenge 2</line-item>
                <question type="Text" name="Target" title="Please enter your Challenge target" mandatory="N"/>

                <question type="Select" name="Units" title="Units">
                    <answer value="1" text="units"/>
                    <answer value="100" text="Hundred"/>
                    <answer value="1000" text="Thousand"/>
                    <answer value="1000000" text="Million"/>
                </question>
            </multi-question>
            
            <question type="MultiSelect" name="Mobility Region" title="Mobility Region">
                <answer value="Any Region" text="Any Region"/>
                <answer value="Europe Main" text="Europe Main"/>
                <answer value="Europe Area I" text="Europe Area I"/>
                <answer value="Europe Area II" text="Europe Area II"/>
                <answer value="LAMEA" text="LAMEA"/>
                <answer value="Asia Pacific" text="Asia Pacific"/>
                <answer value="Japan" text="Japan"/>
                <answer value="United States" text="United States"/>
                <answer value="ISMO Central Functions" text="ISMO Central Functions"/>
            </question>

        </group>

        <group name="Group Name for Organisation">
            <question type="Organisation" name="Organisation name" title="Organisation title"/>
        </group>

        <group name="Group Name for Subject Name">
            <question type="Subject" name="Subject Name name" title="Subject Name title"/>
        </group>

        <group name="Group Name for Position Title">
            <question type="Position" name="Position Title name" title="Position title"/>
        </group>

    </questionnaire>

</xml>