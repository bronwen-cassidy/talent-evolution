<?xml version="1.0" encoding="UTF-8"?>
<xml>
    <questionnaire name="Short Leadership Evaluation" title="Short Leadership Evaluation">
        <group name="Leadership Capabilities">
            <question type="Select" name="Rating" title=" " id="leadership_rating">
                <answer value="Development required" text="Development required"/>
                <answer value="Good" text="Good"/>
                <answer value="Exceptional" text="Exceptional"/>
            </question>

            <question type="Text" name="Supporting examples" title=" " id="leadership_rating_examples"/>

            <question type="Select" name="Competency" title=" " id="leadership_competency">
                <answer value="Substandard" text="Substandard"/>
                <answer value="Average" text="Average"/>
                <answer value="Excellent" text="Excellent"/>
            </question>

            <question type="Integer" name="Age" title="Age" id="candidate_age"/>
            <question type="Integer" name="Years In Job" title="Years In Job" id="candidate_yrs_in_job"/>
        </group>

    </questionnaire>
</xml>
