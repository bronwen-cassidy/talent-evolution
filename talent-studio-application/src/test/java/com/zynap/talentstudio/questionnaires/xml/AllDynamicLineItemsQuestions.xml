<?xml version="1.0" encoding="UTF-8"?>
<xml>
    <questionnaire name="AllQuestions" title="allq">

        <group name="General group title">
            <narrative>General xml type narrative within the group</narrative>
        </group>

        <group name="Narrative Group Only">
            <narrative>
                Narrative without a group name groups together with the next narrative directly above with a group name.
            </narrative>
        </group>

        <group name="Rating" title="Rating">
            <multi-question name=" ">
                <line-item dynamic="Y" managerCanDisable="Y">Rating</line-item>
                <question id="1" type="Text" name="Your Name" title="Name"/>
                <question id="2" type="Select" name="Placement Manager Rating" title="Placement Manager Rating" managerWrite="Y">
                    <answer value="Not Achieved" text="Not Achieved"/>
                    <answer value="Partially Achieved" text="Partially Achieved"/>
                    <answer value="Achieved" text="Achieved"/>
                </question>
                <question id="3" type="Date" name="Date of Rating" title="Date of Rating" managerWrite="Y"/>
                <question id="4" type="TextBox" name="Comments" title="Comments" managerWrite="Y"/>
            </multi-question>
        </group>

        <group name="Group Name for Text1">
            <question id="5" type="Text" name="Text Name" title="Text Hover Title" mandatory="Y"/>
        </group>

        <group name="Group Name for Date">
            <question id="7" type="Date" name="Date Name" title="Date Title"/>
        </group>

        <group name="Group Name for Integer">
            <question id="8" type="Integer" name="Integer Name" title="Integer Title"/>
        </group>

        <group name="Group Name for Positive Integer">
            <question id="9" type="PositiveInteger" name="Positive Integer Name" title="Positive Integer Title"/>
        </group>

        <group name="Group Name for Textbox">
            <question id="10" type="TextBox" name="Textbox Name" title="Textbox Title"/>
        </group>

        <group name="Group Name for Select">

            <question id="11" type="Select" name="Select Name" title="Select Title" target="Select Value1" mandatory="Y">
                <answer value="Select Value1" text="Select Text1"/>
                <answer value="Select Value2" text="Select Text2"/>
                <answer value="Select Value3" text="Select Text3"/>
                <answer value="Select Value4" text="Select Text4"/>
                <answer value="Select Value5" text="Select Text5"/>
            </question>
            <question id="Q9" type="Checkbox" name="Q9 Checkbox Name" title="Checkbox Title" target="Checkbox Value1">
                <answer value="Checkbox Value1" text="Checkbox Text1"/>
                <answer value="Checkbox Value2" text="Checkbox Text2"/>
                <answer value="Checkbox Value3" text="Checkbox Text3"/>
                <answer value="Checkbox Value4" text="Checkbox Text4"/>
                <answer value="Checkbox Value5" text="Checkbox Text5"/>
            </question>
        </group>

        <group name="Group Name for MultiSelect">
            <question id="11B" type="MultiSelect" name="Mobility Region" title="Mobility Region">
                <answer value="Any Region" text="Any Region"/>
                <answer value="Europe Main" text="Europe Main"/>
                <answer value="Europe Area I" text="Europe Area I"/>
                <answer value="Europe Area II" text="Europe Area II"/>
                <answer value="LAMEA" text="LAMEA"/>
                <answer value="Asia Pacific" text="Asia Pacific"/>
                <answer value="Japan" text="Japan"/>
                <answer value="United States" text="United States"/>
                <answer value="ISMO Central Functions" text="ISMO Central Functions"/>
            </question>
        </group>

        <group name="Group Name for Radio">

            <question id="12" type="Radio" name="Radio Name" title="Radio Title" target="Radio Value1">
                <answer value="Radio Value1" text="Radio Text1"/>
                <answer value="Radio Value2" text="Radio Text2"/>
                <answer value="Radio Value3" text="Radio Text3"/>
                <answer value="Radio Value4" text="Radio Text4"/>
                <answer value="Radio Value5" text="Radio Text5"/>
            </question>
        </group>

        <group name="Group Name for Multi Question Test1">

            <multi-question name=" ">
                <line-item>Line Item 1</line-item>

                <question id="13" type="Radio" name="Radio Name" title="Radio Title">
                    <answer value="Radio Value 1" text="Radio Text 1"/>
                    <answer value="Radio Value 2" text="Radio Text 2"/>
                    <answer value="Radio Value 3" text="Radio Text 3"/>
                </question>
                <question id="27" type="Text" name="Text Name" title="Text Title"/>
            </multi-question>
        </group>

        

        <group name="Group Name for Organisation">
            <question id="16" type="Organisation" name="Organisation name" title="Organisation title"/>
        </group>

        <group name="Group Name for Subject Name">
            <question id="17" type="Subject" name="Subject Name name" title="Subject Name title"/>
        </group>

        <group name="Group Name for Position Title">
            <question id="18" type="Position" name="Position Title name" title="Position title"/>
        </group>

        <group name="Group Name for Another kind of Multi Question Test">

            <multi-question name="">
                <line-item>Challenge</line-item>
                <question id="19" type="Text" name="Target" title="Please enter your Challenge target" mandatory="N"/>

                <question id="20" type="Select" name="Scaler" title="Scaler">
                    <answer value="1" text="units"/>
                    <answer value="100" text="Hundred"/>
                    <answer value="1000" text="Thousand"/>
                    <answer value="1000000" text="Million"/>
                </question>
                <question id="21" type="Comments" name="Comment" title="Please enter your Challenge comment" mandatory="N"/>
            </multi-question>

            <multi-question name="">
                <line-item>Contribution</line-item>
                <question id="22" type="Text" name="Target" title="Please enter your Contribution target" mandatory="N"/>

                <question id="23" type="Select" name="Scaler" title="Scaler">
                    <answer value="1" text="units"/>
                    <answer value="100" text="Hundred"/>
                    <answer value="1000" text="Thousand"/>
                    <answer value="1000000" text="Million"/>
                </question>
                <question id="29" type="Text" name="Comment" title="Please enter your Contribution comment" mandatory="N"/>
            </multi-question>

            <multi-question name="">
                <line-item>Achievement</line-item>

                <question id="24" type="Text" name="Target" title="Please enter your Achievement target" mandatory="N"/>

                <question id="25" type="Select" name="Scaler" title="Scaler">
                    <answer value="1" text="units"/>
                    <answer value="100" text="Hundred"/>
                    <answer value="1000" text="Thousand"/>
                    <answer value="1000000" text="Million"/>
                </question>

                <question id="26" type="Text" name="Comment" title="Please enter your Achievement comment" mandatory="N"/>

            </multi-question>
        </group>

        <group name="Last Updated" title="Last Updated">
            <question id="40" type="LastUpdated" name="Last Updated (DateTime)" function="lastupdate" format="DateTime"/>
            <question id="41" type="LastUpdatedBy" name="Last Updated (DateTimeUser)" function="lastupdate" format="datetimeuser"/>            
        </group>

        <group name="Sum" title="Sum">

            <question id="32" type="Select" name="A" title="A">
                <answer value="1" text="1"/>
                <answer value="2" text="2"/>
                <answer value="3" text="3"/>
            </question>

            <question id="33" type="Select" name="B" title="B">
                <answer value="1" text="1"/>
                <answer value="2" text="2"/>
                <answer value="3" text="3"/>
            </question>

            <question id="34" type="Select" name="C" title="C">
                <answer value="1" text="1"/>
                <answer value="2" text="2"/>
                <answer value="3" text="3"/>
            </question>

            <question id="35" type="Select" name="D" title="D">
                <answer value="1" text="1"/>
                <answer value="2" text="2"/>
                <answer value="3" text="3"/>
            </question>

            <question id="36" type="Sum" name="Total" title="Total">
                <operand source="32"/>
                <operand source="33"/>
                <operand source="34"/>
                <operand source="35"/>
            </question>

        </group>

        <group name="EnumMapping - One Level" title="EnumMapping - One Level">

            <question id="37" type="EnumMapping" name="EnumMapping - One Level" title="EnumMapping - One Level">
                <mapping upperbound="4" value="Low" text="Low" source="36"/>
                <mapping lowerbound="5" upperbound="8" value="Medium" text="Medium" source="36"/>
                <mapping lowerbound="9" value="High" text="High" source="36"/>
            </question>

        </group>

        <group name="EnumMapping - Two Levels" title="EnumMapping - Two Levels">

            <question id="51" type="Select" name="Final Performance Rating" title="Final Performance Rating">
                <answer value="1" text="1"/>
                <answer value="2" text="2"/>
                <answer value="3" text="3"/>
                <answer value="4" text="4"/>
                <answer value="5" text="5"/>
            </question>

            <question id="52" type="EnumMapping" name="EnumMapping - Two Levels" title="EnumMapping - Two Levels">
                <mapping upperbound="4" value=" " text=" " source="36">
                    <mapping upperbound="2" value="Fall within low and score 1 or 2 within Final Performance Rating" text="Poor Performer" source="51"/>
                    <mapping lowerbound="3" upperbound="3" value="Fall within low and score 3 within Final Performance Rating" text="Good Solid Performer" source="51"/>
                </mapping>
                <mapping lowerbound="5" upperbound="8" value=" " text=" " source="36">
                    <mapping upperbound="2" value="Fall within medium and score 1 or 2 within Final Performance Rating" text="Under Performer" source="51"/>
                    <mapping lowerbound="3" upperbound="3" value="Fall within medium and score 3 within Final Performance Rating" text="Good Solid Performer (with Potential)" source="51"/>
                </mapping>
                <mapping lowerbound="9" value=" " text=" " source="36">
                    <mapping upperbound="2" value="Fall within high and score 1 or 2 within Final Performance Rating" text="Under Performer (with High Potential)" source="51"/>
                    <mapping lowerbound="3" upperbound="3" value="Fall within high and score 3 within Final Performance Rating" text="Good Solid Performer (with High Potential)" source="51"/>
                </mapping>
            </question>
        </group>

        <group name="Dynamic line-item" title="Dynamic line-item">
            <multi-question name=" ">
                <line-item dynamic="Y">Dynamic line-item</line-item>
                <question id="54" type="Text" name="Country" title="Country"/>
                <question id="55" type="PositiveInteger" name="Duration" title="Duration"/>
                <question id="56" type="PositiveInteger" name="Years" title="Years"/>
                <question id="57" type="Text" name="Working Language" title="Working Language"/>
                <question id="58" type="TextBox" name="Additional Comments" title="Additional Comments"/>
                
                <question id="59"  type="Date" name="Some Date" title="Date of Rating" managerWrite="Y"/>
                
                <question id="60" type="Select" name="A" title="A">
                  <answer value="1" text="1"/>
                  <answer value="2" text="2"/>
                  <answer value="3" text="3"/>
                </question>
                
            </multi-question>
        </group>

    </questionnaire>
</xml>
